{
    "env": {
        "browser": true,
        "node": true,
        "commonjs": true
    },
    "parser": "babel-eslint",
    "extends": ["eslint:recommended","plugin:react/recommended"],
    "parserOptions": {
        "ecmaVersion": 2016,
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true
        },
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "semi": [
            "error",
            "always"
        ],
        "no-console": "off",
        "react/jsx-uses-vars": "error"
    }
}
